#include <stdio.h>

int main()
{
    int num1, num2, num3, num4, num5, oddCount=0;
    scanf("%d %d %d %d %d", &num1, &num2, &num3, &num4, &num5);
    if(num1%2 != 0)
    {
        oddCount++;
    }
    if(num2%2 != 0)
    {
        oddCount++;
    }
    if(num3%2 != 0)
    {
        oddCount++;
    }
    if(num4%2 != 0)
    {
        oddCount++;
    }
    if(num5%2 != 0)
    {
        oddCount++;
    }
    printf("%d", oddCount);
}
